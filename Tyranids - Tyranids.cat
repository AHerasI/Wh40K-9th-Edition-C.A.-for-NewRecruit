    <selectionEntry id="2763-5bb5-dcb4-e950" name=" Synaptic Imperatives" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="6b8b-519c-699b-da3a" type="max"/>
