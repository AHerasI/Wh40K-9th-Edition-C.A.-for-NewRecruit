          </modifiers>
          <characteristics>
            <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">Improve the Armour Penetration characteristic of that attack to -3.</characteristic>
          </characteristics>
        </profile>
        <profile id="cc66-935c-c1db-6293" name="Heavy Ammo - Diseased Heavy Bolts" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
          <modifiers>
            <modifier type="set" field="hidden" value="true">
