      <infoGroups>
        <infoGroup id="d4ae-5c35-496d-ed68" name="Angels of Death" hidden="false">
          <modifiers>
            <modifier type="set" field="hidden" value="true">
              <conditionGroups>
                <conditionGroup type="and">
                  <conditions>
                    <condition field="selections" scope="force" value="0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="2c5f-769b-663f-092b" type="equalTo"/>
