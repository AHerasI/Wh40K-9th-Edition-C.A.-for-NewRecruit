          </modifiers>
          <characteristics>
            <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">Add 1 to the Strength and Damage characteristics of that attack.</characteristic>
          </characteristics>
        </profile>
        <profile id="4933-9096-6d95-8790" name="Heavy Ammo - Screaming Bolts" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
          <modifiers>
            <modifier type="set" field="hidden" value="true">
