            <cost name="RP" typeId="ed03-25cc-2681-bdce" value="0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="5c36-2bae-5d4e-597a" name="1CP Faith is our shield" hidden="true" collective="false" import="true" type="upgrade">
