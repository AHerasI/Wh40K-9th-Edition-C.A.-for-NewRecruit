          <selectionEntries>
            <selectionEntry id="18b5-2141-9518-b930" name="2CP Point-blank Fusillade" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="bfe6-47c8-b973-b4be" type="max"/>
