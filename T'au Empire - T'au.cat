          <characteristics>
            <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">Each time an attack is allocated to a AUXILIARY unit of a BORK&apos;AN Detachment, add 1 to the Saving throw.</characteristic>
          </characteristics>
        </profile>
        <profile id="6218-ac6e-fefa-d5b7" name="CQB Mastery" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
          <modifiers>
            <modifier type="set" field="hidden" value="true">
              <comment>Enclaves</comment>
