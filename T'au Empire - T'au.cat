          <selectionEntries>
            <selectionEntry id="f654-6879-e2b6-eee0" name="1CP Emergency dispensation" hidden="true" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="7b95-e971-26c7-cd3d" type="max"/>
