                    <conditionGroup type="or">
                      <conditions>
                        <condition field="selections" scope="parent" value="0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="5d6c-12da-407d-ff32" type="instanceOf"/>
                        <condition field="selections" scope="ancestor" value="0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="5d6c-12da-407d-ff32" type="instanceOf"/>
                      </conditions>
