        <rule id="d82b-0672-754e-8e97" name="Plage weapon" hidden="false">
          <description>Re-roll wound rolls of 1 against units without the VEHICLE, TITANIC or INORGANIC Keywords.</description>
        </rule>
      </rules>
    </categoryEntry>
